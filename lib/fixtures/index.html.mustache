<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <title>{{title}}</title>
    <meta name="viewport" content="width=device-width">
    <meta name="apple-mobile-web-app-title" content="{{title}}">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="format-detection" content="telephone=no">
  </head>
  <body>
    <div id="app"></div>
    <script>
        var cb = function() {
            var css = ['//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css'];
            {{#cssFils}}
            css.push('{{{.}}}');
            {{/cssFils}}
            var h = document.getElementsByTagName('head')[0], l;
            var local = window.location.protocol === 'file:';
            css.forEach(function (f) {
                l = document.createElement('link');
                l.rel = 'stylesheet';
                if (local && f.substr(0, 2) === '//') {
                  l.href = 'http://' + f;
                } else {
                  l.href = f;
                }
                h.parentNode.insertBefore(l, h);
            });
        };
        var raf = requestAnimationFrame || mozRequestAnimationFrame ||
                webkitRequestAnimationFrame || msRequestAnimationFrame;
        if (raf) raf(cb);
        else window.addEventListener('load', cb);
    </script>
    <script>
      window.config = {
        base: '{{{root}}}',
        hashbang: {{hashbang}}
      }
    </script>
    {{#jsFils}}
      <script src="{{{.}}}"></script>
    {{/jsFils}}
    <script src="src/vendor.bundle.js"></script>
    <script src="src/bundle.js"></script>
  </body>
</html>
